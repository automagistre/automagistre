{# @var item \App\Storage\Entity\Motion #}

{% if item.source.type.isOrder() %}
    <a href="{{ easyadmin_path_by_id(item.source.toIdentifier, 'show', {'referer': true}) }}">
        Заказ {{ item.source.toIdentifier|display_name }}
    </a>
{% elseif item.source.type.isIncome() %}
    <a href="{{ easyadmin_path('Income', 'part', {'income_part_id': item.source.id.toString, 'referer': true}) }}">
        Приход от {{ item.source.toIdentifier|display_name }}
    </a>
{% elseif item.source.type.isManual() %}
    {{ item.source.toIdentifier|display_name }}
{% elseif item.source.type.isInventorization() %}
    <a href="{{ easyadmin_path('Inventorization', 'show', {'id': item.source.id.toString, 'referer': true}) }}">
        Инвентаризация
    </a>
{% endif %}
