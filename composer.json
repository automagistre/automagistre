{
    "name": "automagistre/automagistre",
    "type": "project",
    "description": "Main project of Automagistre LLC",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pcntl": "*",
        "ext-sockets": "*",
        "ext-uuid": "*",
        "adamgaskins/barcoder": "^0.1.0",
        "beberlei/doctrineextensions": "^1.2",
        "bronhy/google-my-business-php-client": "^4.7",
        "clue/socket-raw": "^1.4",
        "cron/cron-bundle": "^2.4",
        "datto/json-rpc": "^6.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/migrations": "^3.0",
        "doctrine/orm": "^2.6",
        "easycorp/easyadmin-bundle": "^2",
        "lcobucci/jwt": "^4.0",
        "moneyphp/money": "^3.2",
        "nelmio/cors-bundle": "^2.0",
        "nsq/nsq-bundle": "^0.6.0",
        "nyholm/psr7": "^1.3",
        "odolbeau/phone-number-bundle": "^3.0",
        "oro/doctrine-extensions": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpinnacle/buffer": "^1.0",
        "premier/enum": "^2.0",
        "premier/identifier": "^0.7.0",
        "premier/markdown-builder": "^0.8",
        "ramsey/uuid-doctrine": "^1.6",
        "sensio/framework-extra-bundle": "^6.0",
        "sentry/sentry-symfony": "^4.0",
        "symfony/asset": "^5.0",
        "symfony/console": "^5.0",
        "symfony/doctrine-bridge": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/error-handler": "^5.0",
        "symfony/event-dispatcher": "^5.0",
        "symfony/event-dispatcher-contracts": "^2.0",
        "symfony/expression-language": "^5.0",
        "symfony/flex": "^1.9",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/mailer": "^5.0",
        "symfony/messenger": "^5.0",
        "symfony/monolog-bundle": "^3.3",
        "symfony/process": "^5.0",
        "symfony/property-access": "^5.0",
        "symfony/property-info": "^5.0",
        "symfony/proxy-manager-bridge": "^5.0",
        "symfony/routing": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/security-core": "^5.0",
        "symfony/security-guard": "^5.0",
        "symfony/security-http": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/string": "^5.0",
        "symfony/templating": "^5.0",
        "symfony/translation": "^5.0",
        "symfony/twig-bridge": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/yaml": "^5.0",
        "twig/intl-extra": "^3.0",
        "wapmorgan/morphos": "^3.2",
        "webonyx/graphql-php": "^14.4"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "ralouphie/getallheaders": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-uuid": "*"
    },
    "require-dev": {
        "brianium/paratest": "^6.0",
        "dama/doctrine-test-bundle": "^6.2",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "ergebnis/composer-normalize": "^2.5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "infection/infection": "^0.22.1",
        "phpmetrics/phpmetrics": "^2.6",
        "phpstan/phpstan": "^0.12.0",
        "phpstan/phpstan-doctrine": "^0.12.0",
        "phpstan/phpstan-phpunit": "^0.12.0",
        "phpstan/phpstan-strict-rules": "^0.12.0",
        "phpstan/phpstan-symfony": "^0.12.0",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-phpunit": "^0.15.0",
        "psalm/plugin-symfony": "^2.1",
        "rector/rector-prefixed": "^0.10.21",
        "symfony/browser-kit": "^5.0",
        "symfony/css-selector": "^5.0",
        "symfony/debug-bundle": "^5.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/requirements-checker": "^2.0",
        "symfony/web-profiler-bundle": "^5.0",
        "vimeo/psalm": "^4.3",
        "weirdan/doctrine-psalm-plugin": "^1.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "src",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "scripts": {
        "cache": "@warmup",
        "database": [
            "bin/console doctrine:query:sql --quiet \"SELECT pg_terminate_backend(pg_stat_activity.pid) FROM pg_stat_activity WHERE pg_stat_activity.datname = '${DB_NAME:-db}' AND pid <> pg_backend_pid();\" || true",
            "bin/console doctrine:database:drop --if-exists --force",
            "bin/console doctrine:database:create"
        ],
        "fixtures": "bin/console doctrine:fixtures:load --no-interaction",
        "migration": "bin/console doctrine:migration:migrate --allow-no-migration --no-interaction",
        "migration-diff": [
            "bin/console doctrine:migration:diff --formatted",
            "@php-cs-fixer"
        ],
        "migration-diff-dry": [
            "bin/console doctrine:schema:update --dump-sql --no-interaction",
            "@php-cs-fixer"
        ],
        "migration-generate": [
            "bin/console doctrine:migrations:generate",
            "@php-cs-fixer"
        ],
        "norm": "composer normalize",
        "paratest": "vendor/bin/paratest --runner WrapperRunner --log-junit=var/junit.xml",
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix",
        "phpmetrics": "vendor/bin/phpmetrics --junit=var/junit.xml  --report-html=public/phpmetrics --exclude src/Migrations src",
        "phpstan": "vendor/bin/phpstan --no-interaction --memory-limit=-1",
        "phpstan-baseline": "vendor/bin/phpstan --no-interaction --memory-limit=-1 --generate-baseline",
        "phpunit": "vendor/bin/phpunit --testdox",
        "psalm": "vendor/bin/psalm --no-cache --show-info=false",
        "psalm-baseline": "vendor/bin/psalm --no-cache --update-baseline --set-baseline=psalm-baseline.xml",
        "rector": "vendor/bin/rector",
        "schema": "bin/console doctrine:schema:validate",
        "symfony-requirements": "vendor/bin/requirements-checker APP_ENV=prod APP_DEBUG=0",
        "test": [
            "@putenv APP_ENV=test",
            "@norm",
            "rm -rf var/log/test.log",
            "@warmup",
            "@php-cs-fixer",
            "@phpstan",
            "@psalm",
            "@putenv DB_NAME=db_test",
            "@database",
            "@migration",
            "@schema",
            "@fixtures",
            "@paratest",
            "@putenv APP_ENV=prod",
            "@warmup",
            "@psalm-baseline"
        ],
        "warmup": [
            "rm -rf var/cache/${APP_ENV}",
            "bin/console cache:clear"
        ]
    }
}
