{
    "name": "automagistre/automagistre",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "csa/guzzle-bundle": "^3.0.1",
        "doctrine/doctrine-bundle": "^1.8.1",
        "doctrine/doctrine-cache-bundle": "^1.3.2",
        "doctrine/doctrine-migrations-bundle": "^1.3.1",
        "doctrine/orm": "^2.6.0",
        "grachevko/enum": "^0.10",
        "javiereguiluz/easyadmin-bundle": "1.17.9",
        "misd/phone-number-bundle": "^1.3.2",
        "moneyphp/money": "^3.1.1",
        "sensio/framework-extra-bundle": "^5.1.4",
        "sensiolabs/security-checker": "^4.1.7",
        "sentry/sentry-symfony": "^2.0",
        "symfony/asset": "^4.0.3",
        "symfony/console": "^4.0.3",
        "symfony/expression-language": "^4.0.3",
        "symfony/flex": "^1.0.66",
        "symfony/form": "^4.0.3",
        "symfony/framework-bundle": "^4.0.3",
        "symfony/monolog-bundle": "^3.1.2",
        "symfony/security-bundle": "^4.0.3",
        "symfony/serializer-pack": "^1.0.1",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "^4.0.3",
        "symfony/translation": "^4.0.3",
        "symfony/twig-bundle": "^4.0.3",
        "symfony/yaml": "^4.0.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10.0",
        "lookyman/phpstan-symfony": "^0.4.1",
        "phpstan/phpstan": "^0.9.1",
        "phpstan/phpstan-doctrine": "^0.9.1",
        "phpstan/phpstan-phpunit": "^0.9.3",
        "phpunit/phpunit": "^6.5.5",
        "symfony/debug-bundle": "^4.0.3",
        "symfony/dotenv": "^4.0.3",
        "symfony/requirements-checker": "^1.0",
        "symfony/web-profiler-bundle": "^4.0.3"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script",
            "requirements-checker": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "src",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
