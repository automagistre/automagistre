{
    "name": "automagistre/automagistre",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "csa/guzzle-bundle": "^3.0.1",
        "doctrine/doctrine-bundle": "^1.8.1",
        "doctrine/doctrine-cache-bundle": "^1.3.2",
        "doctrine/doctrine-migrations-bundle": "^1.3.1",
        "doctrine/orm": "^2.6.0",
        "grachevko/enum": "^0.10",
        "javiereguiluz/easyadmin-bundle": "1.17.9",
        "misd/phone-number-bundle": "^1.3.2",
        "moneyphp/money": "^3.1.1",
        "sensio/distribution-bundle": "^5.0.21",
        "sensio/framework-extra-bundle": "^5.1.4",
        "sentry/sentry-symfony": "^2.0",
        "symfony/monolog-bundle": "^3.1.2",
        "symfony/swiftmailer-bundle": "^3.1.6",
        "symfony/symfony": "^3.4.3",
        "twig/twig": "^2.4.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10.0",
        "lookyman/phpstan-symfony": "^0.4.1",
        "phpstan/phpstan": "^0.9.1",
        "phpstan/phpstan-doctrine": "^0.9.1",
        "phpstan/phpstan-phpunit": "^0.9.3",
        "phpunit/phpunit": "^6.5.5",
        "sensio/generator-bundle": "^3.1.7",
        "symfony/dotenv": "^3.3"
    },
    "scripts": {
        "symfony-scripts": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "src",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
