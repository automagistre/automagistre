{
    "name": "automagistre/automagistre",
    "description": "Main project of Automagistre LLC",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.3",
        "beberlei/doctrineextensions": "^1.2",
        "csa/guzzle-bundle": "^3.1",
        "doctrine/doctrine-bundle": "^1.11",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.6",
        "easycorp/easyadmin-bundle": "^v2.1",
        "endroid/qr-code-bundle": "^3.3",
        "facile-it/doctrine-mysql-come-back": "^1.7",
        "grachevko/enum": "^0.14",
        "long-running/long-running": "^0.3",
        "misd/phone-number-bundle": "^1.3",
        "moneyphp/money": "^3.2",
        "ocramius/doctrine-batch-utils": "^2.0",
        "sensio/framework-extra-bundle": "^5.3",
        "sensiolabs/security-checker": "^6.0",
        "sentry/sentry-symfony": "^3.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "^4.2",
        "symfony/console": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/event-dispatcher": "^4.2",
        "symfony/expression-language": "^4.2",
        "symfony/flex": "^1.2",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/http-kernel": "4.3.0",
        "symfony/monolog-bundle": "^3.3",
        "symfony/security-bundle": "^4.2",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "^4.2",
        "symfony/translation": "^4.2",
        "symfony/twig-bundle": "^4.2",
        "symfony/yaml": "^4.2",
        "wapmorgan/morphos": "^3.2"
    },
    "require-dev": {
        "brianium/paratest": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.15",
        "pedrotroller/php-cs-custom-fixer": "^2.17",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-doctrine": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpstan/phpstan-symfony": "^0.11.5",
        "phpunit/phpunit": "^8.1",
        "psalm/plugin-phpunit": "^0.6",
        "symfony/browser-kit": "^4.2",
        "symfony/debug-bundle": "^4.2",
        "symfony/phpunit-bridge": "^4.2",
        "symfony/requirements-checker": "^1.1",
        "symfony/web-profiler-bundle": "^4.2",
        "symfony/web-server-bundle": "^4.2",
        "vimeo/psalm": "^3.2",
        "weirdan/doctrine-psalm-plugin": "^0.4"
    },
    "scripts": {
        "auto-scripts": {
            "cache:warmup": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "src",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public",
        "symfony-tests-dir": "tests"
    },
    "config": {
        "cache-dir": "/var/cache/composer",
        "preferred-install": "dist",
        "sort-packages": true
    }
}
