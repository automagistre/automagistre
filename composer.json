{
    "name": "automagistre/automagistre",
    "description": "Main project of Automagistre LLC",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "beberlei/doctrineextensions": "^1.2",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.6",
        "easycorp/easyadmin-bundle": "^2.3",
        "endroid/qr-code-bundle": "^3.3",
        "lcobucci/jwt": "^3.3",
        "long-running/long-running": "^0.4.1",
        "moneyphp/money": "^3.2",
        "ocramius/doctrine-batch-utils": "^2.0",
        "odolbeau/phone-number-bundle": "^3.0",
        "oro/doctrine-extensions": "^1.3",
        "premier/enum": "^2.0",
        "sensio/framework-extra-bundle": "^5.3",
        "sensiolabs/security-checker": "^6.0",
        "sentry/sentry-symfony": "^3.0",
        "symfony/asset": "^5.0",
        "symfony/console": "^5.0",
        "symfony/doctrine-bridge": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/error-handler": "^5.0",
        "symfony/event-dispatcher": "^5.0",
        "symfony/event-dispatcher-contracts": "^2.0",
        "symfony/expression-language": "^5.0",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/mercure-bundle": "^0.2.3",
        "symfony/monolog-bundle": "^3.3",
        "symfony/process": "^5.0",
        "symfony/routing": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/security-core": "^5.0",
        "symfony/security-guard": "^5.0",
        "symfony/security-http": "^5.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "^5.0",
        "symfony/translation": "^5.0",
        "symfony/twig-bridge": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/yaml": "^5.0",
        "twig/intl-extra": "^3.0",
        "wapmorgan/morphos": "^3.2"
    },
    "require-dev": {
        "brianium/paratest": "^4.0",
        "dama/doctrine-test-bundle": "^6.2",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.15",
        "fzaninotto/faker": "^1.9",
        "pedrotroller/php-cs-custom-fixer": "^2.17",
        "phpstan/phpstan": "^0.12.0",
        "phpstan/phpstan-doctrine": "^0.12.0",
        "phpstan/phpstan-phpunit": "^0.12.0",
        "phpstan/phpstan-strict-rules": "^0.12.0",
        "phpstan/phpstan-symfony": "^0.12.0",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-phpunit": "^0.9.0",
        "symfony/browser-kit": "^5.0",
        "symfony/css-selector": "^5.0",
        "symfony/debug-bundle": "^5.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/requirements-checker": "^1.1",
        "symfony/web-profiler-bundle": "^5.0",
        "vimeo/psalm": "^3.8",
        "weirdan/doctrine-psalm-plugin": "^0.10.0"
    },
    "scripts": {
        "auto-scripts": [
            "bin/console cache:warmup",
            "bin/console assets:install public",
            "rm -rf config/routes/endroid_qr_code.yaml"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "src",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public",
        "symfony-tests-dir": "tests"
    },
    "config": {
        "cache-dir": "/var/cache/composer",
        "preferred-install": "dist",
        "sort-packages": true
    }
}
