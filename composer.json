{
    "name": "automagistre/automagistre",
    "description": "Main project of Automagistre LLC",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pcntl": "*",
        "ext-simplexml": "*",
        "ext-sockets": "*",
        "ext-uuid": "*",
        "adamgaskins/barcoder": "^0.1.0",
        "artprima/prometheus-metrics-bundle": "^1.12",
        "beberlei/doctrineextensions": "^1.2",
        "bronhy/google-my-business-php-client": "^4.7",
        "clue/socket-raw": "^1.4",
        "cron/cron-bundle": "^2.10",
        "datto/json-rpc": "^6.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/migrations": "^3.0",
        "doctrine/orm": "^2.6",
        "google/apiclient": "^2.12",
        "guzzlehttp/guzzle": "^7.4",
        "lcobucci/jwt": "^4.1",
        "michaelpetri/typed-input": "^1.1",
        "mohammad-waleed/keycloak-admin-client": "^0.28.0",
        "moneyphp/money": "^4.0",
        "nsq/nsq-bundle": "^0.8.0",
        "nyholm/psr7": "^1.3",
        "odolbeau/phone-number-bundle": "^3.0",
        "oro/doctrine-extensions": "^2.0",
        "pagerfanta/pagerfanta": "^3.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpinnacle/buffer": "^1.0",
        "premier/enum": "^2.2",
        "premier/identifier": "^0.7.0",
        "premier/markdown-builder": "^0.8",
        "ramsey/uuid-doctrine": "^1.6",
        "sensio/framework-extra-bundle": "^6.0",
        "sentry/sentry-symfony": "^4.0",
        "stevenmaguire/oauth2-keycloak": "dev-hotfix",
        "symfony/asset": "6.0.*",
        "symfony/console": "6.0.*",
        "symfony/doctrine-bridge": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/error-handler": "6.0.*",
        "symfony/event-dispatcher": "6.0.*",
        "symfony/event-dispatcher-contracts": "^3.0",
        "symfony/expression-language": "6.0.*",
        "symfony/flex": "^2.1",
        "symfony/form": "6.0.*",
        "symfony/framework-bundle": "6.0.*",
        "symfony/http-kernel": "6.0.*",
        "symfony/mailer": "6.0.*",
        "symfony/messenger": "6.0.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/process": "6.0.*",
        "symfony/property-access": "6.0.*",
        "symfony/property-info": "6.0.*",
        "symfony/proxy-manager-bridge": "6.0.*",
        "symfony/routing": "6.0.*",
        "symfony/security-bundle": "6.0.*",
        "symfony/serializer": "6.0.*",
        "symfony/string": "6.0.*",
        "symfony/templating": "6.0.*",
        "symfony/translation": "6.0.*",
        "symfony/twig-bridge": "6.0.*",
        "symfony/twig-bundle": "6.0.*",
        "symfony/validator": "6.0.*",
        "symfony/yaml": "6.0.*",
        "thecodingmachine/safe8": "^0.1.0",
        "twig/intl-extra": "^3.0",
        "wapmorgan/morphos": "^3.2",
        "webonyx/graphql-php": "^14.4"
    },
    "require-dev": {
        "brianium/paratest": "^6.4",
        "dama/doctrine-test-bundle": "^6.7",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "ergebnis/composer-normalize": "^2.23",
        "friendsofphp/php-cs-fixer": "^3.5",
        "infection/infection": "^0.26.4",
        "phpmetrics/phpmetrics": "^2.7",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-doctrine": "^1.2",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpstan/phpstan-symfony": "^1.1",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16.1",
        "psalm/plugin-symfony": "^3.1",
        "symfony/browser-kit": "6.0.*",
        "symfony/css-selector": "6.0.*",
        "symfony/debug-bundle": "6.0.*",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/requirements-checker": "^2.0",
        "symfony/web-profiler-bundle": "6.0.*",
        "vimeo/psalm": "^4.19",
        "weirdan/doctrine-psalm-plugin": "^2.2"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "ralouphie/getallheaders": "*",
        "symfony/amqp-messenger": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-uuid": "*",
        "thecodingmachine/safe": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/grachevko/oauth2-keycloak.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "EasyCorp\\Bundle\\EasyAdminBundle\\": "easyadmin/"
        },
        "files": [
            "fork/Generator.php",
            "fork/SqlGenerator.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true,
            "symfony/flex": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "require": "6.0.*"
        },
        "symfony-app-dir": "src",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public"
    },
    "scripts": {
        "cache": "@warmup",
        "cache-prod": [
            "@putenv APP_ENV=prod",
            "@putenv APP_DEBUG=0",
            "@warmup"
        ],
        "fixtures": "bin/console doctrine:fixtures:load --no-interaction",
        "migration-diff": [
            "bin/console doctrine:migration:diff --formatted"
        ],
        "migration-diff-dry": "bin/console doctrine:schema:update --dump-sql --no-interaction",
        "norm": "composer normalize",
        "paratest": "vendor/bin/paratest --log-junit=var/junit.xml",
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix",
        "phpmetrics": "vendor/bin/phpmetrics --junit=var/junit.xml  --report-html=public/phpmetrics --exclude src/Migrations src",
        "phpstan": "vendor/bin/phpstan --no-interaction --memory-limit=-1",
        "phpstan-baseline": "vendor/bin/phpstan --no-interaction --memory-limit=-1 --generate-baseline",
        "phpunit": "vendor/bin/phpunit --testdox",
        "psalm": "vendor/bin/psalm --no-cache --show-info=false",
        "psalm-baseline": "vendor/bin/psalm --no-cache --update-baseline --set-baseline=psalm-baseline.xml",
        "schema": "bin/console doctrine:schema:validate",
        "symfony-requirements": "vendor/bin/requirements-checker APP_ENV=prod APP_DEBUG=0",
        "test": [
            "@putenv APP_ENV=test",
            "@norm",
            "rm -rf var/log/test.log",
            "@warmup",
            "@php-cs-fixer",
            "@phpstan",
            "@psalm",
            "@putenv DB_NAME=db_test",
            "@schema",
            "@fixtures",
            "@paratest",
            "@cache-prod",
            "@psalm-baseline"
        ],
        "warmup": [
            "rm -rf var/cache/${APP_ENV}",
            "bin/console cache:clear"
        ]
    }
}
