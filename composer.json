{
    "name": "automagistre/automagistre",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.2",
        "beberlei/DoctrineExtensions": "^1.1",
        "csa/guzzle-bundle": "^3.1.0",
        "doctrine/doctrine-bundle": "^1.9.1",
        "doctrine/doctrine-cache-bundle": "^1.3.5",
        "doctrine/doctrine-migrations-bundle": "^1.3.1",
        "doctrine/orm": "^2.6.2",
        "easycorp/easyadmin-bundle": "^1.17.14",
        "grachevko/enum": "^0.13.1",
        "misd/phone-number-bundle": "^1.3.2",
        "moneyphp/money": "^3.1.3",
        "sensio/framework-extra-bundle": "^5.2.2",
        "sensiolabs/security-checker": "^5.0.1",
        "sentry/sentry-symfony": "^2.1",
        "stof/doctrine-extensions-bundle": "^1.3.0",
        "symfony/asset": "^4.1.7",
        "symfony/console": "^4.1.7",
        "symfony/dotenv": "^4.1.7",
        "symfony/event-dispatcher": "^4.1.7",
        "symfony/expression-language": "^4.1.7",
        "symfony/flex": "^1.1.1",
        "symfony/form": "^4.1.7",
        "symfony/framework-bundle": "^4.1.7",
        "symfony/monolog-bundle": "^3.3.1",
        "symfony/security-bundle": "^4.1.7",
        "symfony/serializer-pack": "^1.0.1",
        "symfony/swiftmailer-bundle": "^3.2.4",
        "symfony/templating": "^4.1.7",
        "symfony/translation": "^4.1.7",
        "symfony/twig-bundle": "^4.1.7",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "^4.1.7"
    },
    "require-dev": {
        "brianium/paratest": "^2.1",
        "friendsofphp/php-cs-fixer": "^2.13.1",
        "pedrotroller/php-cs-custom-fixer": "^2.15.4",
        "phpstan/phpstan": "^0.10.5",
        "phpstan/phpstan-doctrine": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "phpstan/phpstan-strict-rules": "^0.10.1",
        "phpstan/phpstan-symfony": "^0.10.2",
        "phpunit/phpunit": "^7.4.3",
        "symfony/browser-kit": "^4.1.7",
        "symfony/debug-bundle": "^4.1.7",
        "symfony/phpunit-bridge": "^4.1.7",
        "symfony/requirements-checker": "^1.1.2",
        "symfony/web-profiler-bundle": "^4.1.7"
    },
    "scripts": {
        "auto-scripts": {
            "cache:warmup": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "src",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative"
    },
    "config": {
        "apcu-autoloader": true,
        "cache-dir": "/var/cache/composer",
        "preferred-install": "dist",
        "sort-packages": true
    }
}
