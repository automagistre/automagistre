{
    "name": "automagistre/automagistre",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "provide": {
        "ext-ast": "*"
    },
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*",
        "symfony/symfony": "3.4.x-dev",
        "doctrine/orm": "^2.5.6",
        "doctrine/doctrine-bundle": "^1.6.7",
        "doctrine/doctrine-cache-bundle": "^1.3.0",
        "symfony/swiftmailer-bundle": "v3.0.1",
        "symfony/monolog-bundle": "^3.1.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^3.0.25",
        "twig/twig": "^2.3.2",
        "doctrine/doctrine-migrations-bundle": "^1.2.1",
        "ramsey/uuid-doctrine": "^1.3.0",
        "javiereguiluz/easyadmin-bundle": "^1.16.9",
        "grachevko/enum": "^0.8.0",
        "misd/phone-number-bundle": "^1.2.0",
        "egeloen/ckeditor-bundle": "^5.0.1",
        "csa/guzzle-bundle": "^2.2.0",
        "friendsofsymfony/user-bundle": "^2.0.0",
        "moneyphp/money": "^3.0.5",
        "sentry/sentry-symfony": "^0.7.1"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.1.4",
        "friendsofphp/php-cs-fixer": "^2.3.1",
        "phpunit/phpunit": "^6.1.3",
        "phpstan/phpstan": "0.7",
        "phpstan/phpstan-guzzle": "0.7",
        "phpstan/phpstan-doctrine": "0.7",
        "etsy/phan": "^0.9.1"
    },
    "scripts": {
        "symfony-scripts": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "vendor",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative"
    }
}
